<html>

<meta charset="UTF-8">
<head>
    <title><% title %></title>

    <!-- CSS (load bootstrap from a CDN) -->
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <script type="text/javascript" src="/bower/jquery/dist/jquery.js"></script>
    <script type="text/javascript" src="/bower/src/bootstrap-table.css"></script>
    <script type="text/javascript" src="/bower/src/bootstrap-table.js"></script>
    <style>
        body    { padding-top:50px; }
    </style>
</head>
<body>
<h2> <% title %></h2>
<table class=" table table-striped table-bordered" style= " cellpadding:5px; cellspacing:5px;">
    <tr>
        <th>Product</th>
        <th>Count</th>

    </tr>

    <% for(var i = 0;i < results.length;i++) { %>
    <tr>
        <td><%=results[i].coll%></td>
        <td><%=results[i].roww%></td>

    </tr>
    <% } %>

</table>

</body>
</html>